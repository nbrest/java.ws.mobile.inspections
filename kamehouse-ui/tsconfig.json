{
  "compilerOptions": {
    "outDir": "./src/main/webapp",
    "rootDir": "./src/main/typescript",
    "allowJs": false,
    "target": "ES2023",
    "noImplicitAny": false,
    "noUnusedParameters": false,
    "strict": false,
    "noImplicitThis": true,
    "noUncheckedIndexedAccess": true,
    "noUnusedLocals": true,
    "noFallthroughCasesInSwitch": true
  },
  "include": ["./src/main/typescript/**/*"]
}