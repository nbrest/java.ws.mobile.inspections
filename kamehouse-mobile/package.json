{
  "name": "kamehouse-mobile",
  "version": "latest",
  "displayName": "KameHouse",
  "description": "KameHouse Mobile App",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "scan": "tsc --build --force --verbose --pretty",
    "test": "echo \"NO tests defined yet\" && exit 0"
